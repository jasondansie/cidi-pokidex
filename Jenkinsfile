pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building from branch "feature-Dang"..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing from branch "feature-Dang"..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying from branch "feature-Dang"....'
            }
        }
    }
}